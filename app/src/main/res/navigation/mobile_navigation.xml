<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.probit.parkapp.ui.map.MapsFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_maps"
        />

    <fragment
        android:id="@+id/reserva_map"
        android:name="com.probit.parkapp.ui.map.MapsFragment"
        android:label="@string/title_map"


        tools:layout="@layout/fragment_maps"
        />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.probit.parkapp.ui.dashboard.DashboardFragment"
        android:label="@string/title_reservas"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.probit.parkapp.PerfilActivity"
        android:label="@string/perfil"
        tools:layout="@layout/fragment_profile" />
</navigation>
